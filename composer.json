{
    "name": "laravel-liberu/companies",
    "description": "Person management dependency for Laravel Liberu",
    "keywords": [
        "laravel",
        "laravel-liberu",
        "companies",
        "company-management"
    ],
    "homepage": "https://github.com/laravel-liberu/companies",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/addresses": "^5.0",
        "laravel-liberu/enums": "^2.0",
        "laravel-liberu/dynamic-methods": "^2.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/migrator": "^2.1",
        "laravel-liberu/people": "^4.0",
        "laravel-liberu/rememberable": "^3.0",
        "laravel-liberu/searchable": "^2.2",
        "laravel-liberu/track-who": "^2.0",
        "laravel-liberu/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Companies\\": "src/",
            "LaravelLiberu\\Companies\\Database\\Factories\\": "database/factories/",
            "LaravelLiberu\\Companies\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Companies\\AppServiceProvider",
                "LaravelLiberu\\Companies\\AuthServiceProvider",
                "LaravelLiberu\\Companies\\SearchServiceProvider",
                "LaravelLiberu\\Companies\\EnumServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
